            <dict>
                <key>public.filename-extension</key>
                <array>
                    <string>cbr</string>
                </array>
            </dict>
        </dict>
        <dict>
            <key>UTTypeConformsTo</key>
            <array>
                <string>public.archive</string>
            </array>
            <key>UTTypeDescription</key>
            <string>Comic Book 7-Zip Archive</string>
            <key>UTTypeIdentifier</key>
            <string>com.inkypanels.cb7</string>
            <key>UTTypeTagSpecification</key>
            <dict>
                <key>public.filename-extension</key>
                <array>
                    <string>cb7</string>
                </array>
            </dict>
        </dict>
    </array>
    <key>NSFaceIDUsageDescription</key>
    <string>Use Face ID to unlock your secure vault</string>
